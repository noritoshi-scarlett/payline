{
    "name": "noritoshi/payline",
    "description": "This is a simple module for handling logs, e.g. payment transactions.",
    "version": "1.0.2",
    "type": "library",
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "ext-redis": "*"
    },
    "autoload": {
        "psr-4": {
            "Noritoshi\\Payline\\": "payline/src/Payline/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^12.1@dev"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:noritoshi-scarlett/payline.git"
        }
    ]
}
