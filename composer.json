{
    "name": "noritoshi/payline",
    "description": "This is a simple module for handling logs, e.g. payment transactions.",
    "type": "composer-plugin",
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "symfony/dependency-injection": "7.3.x-dev",
        "moneyphp/money": "3.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Payline\\App\\": "payline/src/",
            "Payline\\Test\\": "payline/test/",
            "Payline\\Example\\": "payline/example/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^12.1@dev"
    }
}
